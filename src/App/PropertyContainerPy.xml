<?xml version="1.0" encoding="UTF-8"?>
<GenerateModel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="generateMetaModel_Module.xsd">
  <PythonExport 
    Father="PersistencePy" 
    Name="PropertyContainerPy" 
    TwinPointer="PropertyContainer" 
    Twin="PropertyContainer" 
    Include="App/PropertyContainer.h" 
    Namespace="App" 
    FatherInclude="Base/PersistencePy.h" 
    FatherNamespace="Base">
    <Documentation>
      <Author Licence="LGPL" Name="Juergen Riegel" EMail="FreeCAD@juergen-riegel.net" />
      <UserDocu>This is a Persistence class</UserDocu>
    </Documentation>
    <Methode Name="getPropertyByName">
      <Documentation>
        <UserDocu>getPropertyByName( name )
                  Return the value of a named property.
                  
                  :param name: Name of property.
                  :type name: string
                  
                  :rtype: Depends on the type of property.
                  </UserDocu>
      </Documentation>
    </Methode>
    <Methode Name="getTypeOfProperty">
		  <Documentation>
			  <UserDocu>getTypeOfProperty( name )
                        Return the type of a named property. This can be (Hidden,ReadOnly,Output) or any combination.
                        
                        :param name: Name of property.
                        :type name: string
                        
                        :rtype: string
                        </UserDocu>
		  </Documentation>
	  </Methode>
      <Methode Name="setEditorMode">
            <Documentation>
                <UserDocu>setEditorMode( name, setting ) 
                          Set the behaviour of the property in the property editor.
                          
                          :param name: Name of property.
                          :type name: string
                          :param setting: Default, hidden or read-only
                          :type setting: integer
                          
                          :rtype: None
                          
                          The following settings are possible:
                            0 - default behaviour
                            1 - item is ready-only
                            2 - item is hidden
                </UserDocu>
            </Documentation>
        </Methode>
      <Methode Name="getGroupOfProperty">
		  <Documentation>
			  <UserDocu>getGroupOfProperty( name )
                        Return the name of the group which the class property belongs to.
                        
                        :param name: Name of property.
                        :type name: string
                        
                        For convenience properties can be sorted in different named groups.
                        </UserDocu>
		  </Documentation>
	  </Methode>
	  <Methode Name="getDocumentationOfProperty">
		  <Documentation>
			  <UserDocu>getDocumentationOfProperty( name )
                        Return the documentation string of this class property.
                        
                        :param name: Name of property.
                        :type name: string
                        
                        :rtype: None
                        </UserDocu>
		  </Documentation>
	  </Methode>
    <Attribute Name="PropertiesList" ReadOnly="true">
      <Documentation>
        <UserDocu>PropertiesList
                  List the names of the properties this object has.
                  
                  :rtype: list
                  </UserDocu>
      </Documentation>
      <Parameter Name="PropertiesList" Type="List"/>
    </Attribute>
    <CustomAttributes />
  </PythonExport>
</GenerateModel>
